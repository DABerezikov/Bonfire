<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:Bonfire.ViewModels"
                    xmlns:view="clr-namespace:Bonfire.Views">

    <DataTemplate DataType="{x:Type vm:SeedsViewModel}">
        <view:SeedsWindow DataContext="{Binding}"></view:SeedsWindow>

    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:LibraryEditorViewModel}">
        <view:LibraryEditorWindow DataContext="{Binding}"></view:LibraryEditorWindow>

    </DataTemplate>
    
</ResourceDictionary>